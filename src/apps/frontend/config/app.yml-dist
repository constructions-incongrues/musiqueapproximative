# default values
all:
  autoplay: ${APP_AUTOPLAY}
  domain: ${APP_DOMAIN}
  theme: ${APP_THEME}
  title: ${APP_TITLE}
  urls:
    tracks: //${APP_DOMAIN}/tracks
    secure_player: https://${APP_DOMAIN}/player.swf
  cloudflare:
    api_token: ${CLOUDFLARE_API_TOKEN}
    zone_id: ${CLOUDFLARE_ZONE_ID}
